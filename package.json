{
  "name": "@nextcloud/initial-state",
  "version": "2.2.0",
  "description": "Access data from the nextcloud server-side initial state API within apps.",
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-initial-state#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-initial-state"
  },
  "license": "GPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:cleanup && npm run build:source",
    "build:cleanup": "tsc --build --clean",
    "build:doc": "typedoc --out dist/doc lib/index.ts && touch dist/doc/.nojekyll",
    "build:source": "tsc",
    "lint": "eslint lib test *.ts",
    "lint:fix": "eslint --fix lib test *.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@nextcloud/eslint-config": "^8.4.2",
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "jsdom": "^26.1.0",
    "typedoc": "^0.28.8",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^24.0.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22.0.0",
      "onFail": "error"
    },
    "packageManager": [
      {
        "name": "npm",
        "version": "^10.5.0",
        "onFail": "error"
      }
    ]
  }
}
